{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "TIMING=1 eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "check-deps": "dependency-cruiser -m src",
    "db:migration:create": "typeorm migration:generate migration:create",
    "db:migration:generate": "node --loader ts-node/esm ../../node_modules/.bin/typeorm migration:generate -d ./src/modules/shared/database/database.typeorm.datasource.ts",
    "db:migrate": "node --loader ts-node/esm ../../node_modules/.bin/typeorm migration:run -d ./src/modules/shared/database/database.typeorm.datasource.ts",
    "generate:env": "cp .env.example .env",
    "clean": "rm -rf node_modules dist coverage"
  },
  "dependencies": {
    "@nestjs/common": "10.2.8",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.8",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/swagger": "7.1.14",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.3.1",
    "express-http-context": "1.2.4",
    "nest-winston": "1.9.4",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17",
    "uuid": "9.0.1",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.8",
    "@types/express": "4.17.20",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@types/supertest": "2.0.15",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "dependency-cruiser": "15.1.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-check-file": "2.6.2",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "node-mocks-http": "1.13.0",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "testcontainers": "10.2.2",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
